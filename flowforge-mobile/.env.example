# GitHub OAuth App Client ID (create at https://github.com/settings/developers)
EXPO_PUBLIC_GITHUB_CLIENT_ID=your_client_id_here

# Backend token exchange endpoint
EXPO_PUBLIC_TOKEN_ENDPOINT=https://your-backend.vercel.app/api/auth/token

# Sentry DSN for crash reporting (optional in development)
EXPO_PUBLIC_SENTRY_DSN=https://your_key@sentry.io/your_project
